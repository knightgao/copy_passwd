{
  "manifest_version": 3,
  "name": "密码复制助手",
  "version": "1.0",
  "description": "在密码输入框左侧添加复制按钮，方便快速复制密码",
  "homepage_url": "https://github.com/knightgao/copy_passwd",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 