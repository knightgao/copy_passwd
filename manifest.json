{
  "manifest_version": 3,
  "name": "Password Viewer",
  "version": "1.0",
  "description": "在密码输入框旁添加查看密码按钮",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 